{
    "manifest_version": 3,
    "name": "Random Number Generator",
    "description": "Get random number, straight from the extension",
    "version": "1.0",
    "icons": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png"
      },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "action": {
        "default_popup": "index.html"
    }

  }